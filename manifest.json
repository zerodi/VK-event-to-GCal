{
    "manifest_version": 2,

    "name": "VK Event to Google Calendar",
    "version": "0.2.5",
    "description": "Добавляет информацию о мероприятии из Вконтакте в Календарь Google",

    "icons": {
        "128": "ico/icon128.png",
        "16": "ico/icon16.png",
        "48": "ico/icon48.png"
    },

    "permissions": [
        "contextMenus",
        "tabs",
        "http://www.google.com/*",
        "https://www.google.com/",
        "http://*.vk.com/*",
        "https://*.vk.com/*"],

    "background": {
    "scripts": [
        "background.js"
    ]}
}
