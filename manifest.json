{
    "manifest_version": 2,

    "name": "vk.com rehosting",
    "version": "1.0.3",
    "description": "Rehosts images in vk.com documents",

    "homepage_url": "http://crea7or.spb.ru",

    "icons": {
        "128": "imageinfo-128.png",
        "16": "imageinfo-16.png",
        "48": "imageinfo-48.png"
    },

    "permissions": [ "contextMenus", "tabs", "storage" , "http://*.vk.com/*", "https://*.vk.com/*" ],

    "background": {
    "scripts": [      
      "background.js"
    ]}
}
